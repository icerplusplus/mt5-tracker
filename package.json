{
  "name": "mt5-trading-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "test:env": "tsx scripts/check-env.ts",
    "test:db": "tsx scripts/test-supabase.ts",
    "test:connection": "tsx scripts/test-connection.ts",
    "test:websocket": "tsx scripts/test-websocket.ts",
    "test:ws-connection": "tsx scripts/test-websocket-connection.ts",
    "test:all": "tsx scripts/check-env.ts && tsx scripts/test-supabase.ts",
    "vps:dev": "node vps-server.js",
    "vps:start": "pm2 start vps-server.js --name mt5-api",
    "vps:stop": "pm2 stop mt5-api",
    "vps:restart": "pm2 restart mt5-api",
    "vps:logs": "pm2 logs mt5-api",
    "vps:status": "pm2 status"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "date-fns": "^4.1.0",
    "lightweight-charts": "^4.2.2",
    "lucide-react": "^0.468.0",
    "next": "15.5.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.5.11",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
